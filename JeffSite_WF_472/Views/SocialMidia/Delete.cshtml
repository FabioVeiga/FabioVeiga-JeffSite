@model JeffSite_WF_472.Models.SocialMidia
@{
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
<br>
<h1>@ViewData["Title"]</h1>
<br>

@using (Html.BeginForm("Delete", "SocialMidia"))
{
    @Html.AntiForgeryToken()
<div class="form-group">
    <label>@Html.DisplayNameFor(Model => Model.Name)</label>
    @Html.EditorFor(model => model.Name, new { htmlAttributes = new { @class = "form-control", type = "text", @readonly = "readonly" } })
</div>
<div class="form-group">
    <label>@Html.DisplayNameFor(Model => Model.Url)</label>
    @Html.EditorFor(model => model.Url, new { htmlAttributes = new { @class = "form-control", type = "text", @readonly = "readonly" } })
</div>
    @Html.ActionLink("Voltar", "Index", "SocialMidia")
    <button class="btn btn-danger" type="submit">Salvar</button>
}
